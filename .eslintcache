to localStorage\n          ls.set('nominations', [item]);\n        } else {\n          let newNominations = [...nominations, item]\n          setNominations([...nominations, item])\n\n          // adding to localStorage\n          ls.set('nominations', newNominations);\n        }\n    }\n\n    const removeFromNomination = (index) => {\n        let array = [...nominations]\n\n        if (index !== -1) {\n            array.splice(index, 1);\n            setNominations(array);\n        }\n    }\n\n    // Logic Handles\n    const isButtonDisabled = (item) => {\n        return nominations && (nominations.includes(item) || nominations.length >= 5)\n    }\n\n    useEffect(() => {\n        let myNominations = ls.get('nominations');\n        setNominations(myNominations)\n    }, [])\n  \n  return (\n    <div className=\"app-body\">\n      <div className=\"sub-body\">\n      <h1 className=\"header-main\" >The Shoppies</h1>\n      \n      {/* TODO: MAKE COMPONENT */}\n      <div className=\"input-body\">\n        <input \n          className=\"input-field\"\n          label=\"Search for movies...\"\n          value={searchQuery}\n          placeholder=\"ðŸ”Ž  Search for movies...\"\n          onChange={handleSearchQuery}\n        />\n      </div>\n\n      {\n        searchQuery ?\n        <div className=\"header-sub-body\">\n          <h1 className=\"header-sub\">Results for \"{searchQuery}\"</h1>\n        </div> \n      :\n      null\n      }\n\n      <div className=\"body-components\">\n      <ul className=\"ul-right\">\n        {\n          resultData ? \n          resultData.map((item, index) => {\n            return (\n              <li>\n                <MovieCard \n                  key={index}\n                  index={index}\n                  item={item}  \n                  isButtonDisabled={isButtonDisabled}\n                  addtoNominations={addtoNominations}\n                />\n              </li>\n            )\n          }) \n          :\n          <div>\n            No Data to show\n          </div> \n        }\n      </ul>\n\n      <hr></hr>\n\n      <ul className=\"ul-left\">\n        {\n          !nominations ? \n          <div>\n            No Data to show\n          </div> :\n          nominations.map((item, index) => {\n            return (\n              <li>\n                <MovieCard \n                  key={index}\n                  index={index}\n                  item={item}  \n                  isButtonDisabled={isButtonDisabled}\n                  removeFromNomination={removeFromNomination}\n                />\n              </li>\n            )\n          })\n        }\n        {\n          (nominations!==null && nominations.length > 4) ?         \n            <div>\n              <h1>Cant add anymore stuff....</h1>\n              <h1>Nomination limit is 5 per user</h1>\n            </div> \n        : null\n        }\n      </ul>\n      </div>\n      \n      </div>\n\n    </div>\n  );\n}\n\nexport default App;\n","/Users/sehmim/2021/youtube/shopify-test/src/components/MovieCard.js",[],{"ruleId":"19","replacedBy":"20"},{"ruleId":"21","replacedBy":"22"},{"ruleId":"23","severity":1,"message":"24","line":29,"column":26,"nodeType":"25","endLine":29,"endColumn":37},"no-native-reassign",["26"],"no-negated-in-lhs",["27"],"react-hooks/exhaustive-deps","React Hook useCallback received a function whose dependencies are unknown. Pass an inline function instead.","Identifier","no-global-assign","no-unsafe-negation"]